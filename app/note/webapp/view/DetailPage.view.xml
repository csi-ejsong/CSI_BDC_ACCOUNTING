<mvc:View xmlns:table="sap.ui.table" controllerName="note.controller.DetailPage"
    displayBlock="true"
    xmlns="sap.ui.table"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    xmlns:f="sap.f">
    <m:Page title="DetailPage">
        <f:DynamicPage >
            <f:content>
                <TreeTable 
                    id="TreeTableDetail"
                    rows="{
                        path: 'row>/',
                        parameters: {
                            arrayNames: ['children']
                            }
                        }">
                    <columns>
                    </columns>
                </TreeTable>
            </f:content>
        </f:DynamicPage>
    </m:Page>

</mvc:View>